{
  "name": "tpw",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "clean": "rimraf dist",
    "clean-dev": "rimraf dev && rm -f npm-debug.log*",
    "build-client": "ng build --prod --output-path=dev/client",
    "build-server": "tsc --project src/server/ --outDir dist/",
    "build": "npm run clean && npm run build-server && npm run build-client",
    "build-dev-client": "ng build --output-path=dev/client",
    "build-dev-server": "tsc --project src/server/ --outDir dev/ --sourceMap",
    "build-dev": "npm run clean-dev && npm run build-dev-server & npm run build-dev-client",
    "dev": "npm run build-dev && nodemon --watch dev/server dev/server/app.js & npm run watch",
    "livereload": "$(npm bin)/browser-sync start --proxy 'http://localhost:3000' --files 'dev/client/**/*'",
    "start": "node dist/server/app.js",
    "test": "ng test",
    "watch": "npm run watch-dev-client & npm run watch-dev-server",
    "watch-dev-client": "ng build --watch --output-path=dev/client",
    "watch-dev-server": "tsc --project src/server/ --outDir dev/ --sourceMap --watch"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.0.0",
    "@angular/common": "^4.0.0",
    "@angular/compiler": "^4.0.0",
    "@angular/core": "^4.0.0",
    "@angular/forms": "^4.0.0",
    "@angular/http": "^4.0.0",
    "@angular/material": "^2.0.0-beta.2",
    "@angular/platform-browser": "^4.0.0",
    "@angular/platform-browser-dynamic": "^4.0.0",
    "@angular/platform-server": "^4.0.0",
    "@angular/router": "^4.0.0",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.2",
    "@ngrx/store": "^2.2.1",
    "@ngrx/store-devtools": "^3.2.4",
    "core-js": "^2.4.1",
    "koa-bodyparser": "^2.5.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.1.0",
    "lodash": "^4.17.4",
    "lokijs": "^1.4.3",
    "reselect": "^2.5.4",
    "rxjs": "^5.1.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/compiler-cli": "^4.0.0",
    "@types/jasmine": "2.5.38",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.23",
    "@types/koa-router": "^7.0.22",
    "@types/koa-static": "^2.0.22",
    "@types/lodash": "^4.14.58",
    "@types/lokijs": "^1.2.30",
    "@types/node": "~6.0.60",
    "angular2-template-loader": "^0.6.2",
    "codelyzer": "~2.0.0",
    "electron": "^1.4.15",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "~3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "koa": "^1.2.4",
    "nodemon": "^1.11.0",
    "ngrx-store-freeze": "^0.1.9",
    "ngrx-store-logger": "^0.1.7",
    "rimraf": "^2.5.2",
    "protractor": "~5.1.0",
    "ts-node": "~2.0.0",
    "tslint": "~4.5.0",
    "typescript": "~2.2.1",
    "wallaby-webpack": "0.0.34"
  }
}
